{
  "name": "holdwall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--no-warnings' next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:advanced": "jest --config jest.config.advanced.js",
    "test:advanced:all": "tsx scripts/run-advanced-tests.ts",
    "test:models": "jest __tests__/advanced/ai-models-comprehensive.test.ts",
    "test:algorithms": "jest __tests__/advanced/algorithms-comprehensive.test.ts",
    "test:scenarios": "jest __tests__/advanced/scenarios-comprehensive.test.ts",
    "test:flows": "jest __tests__/advanced/business-flows-comprehensive.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:real-world": "playwright test __tests__/e2e/real-world-scenarios.test.ts",
    "test:load": "tsx __tests__/load/load-test.ts",
    "test:eval": "jest __tests__/evaluation --coverage",
    "eval:check-regression": "tsx scripts/check-eval-regression.ts",
    "eval:run": "tsx scripts/run-evaluation.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx scripts/seed.ts",
    "db:studio": "prisma studio",
    "demo:data": "tsx scripts/demo-data.ts",
    "verify": "node scripts/verify-everything.js",
    "verify:api-keys": "tsx scripts/validate-api-keys.ts",
    "verify:health": "curl -s http://localhost:3000/api/health | jq || echo 'Server not running. Start with: npm run dev'",
    "verify:flows": "curl -s -X POST http://localhost:3000/api/verification/run -H 'Content-Type: application/json' -d '{\"flow\":\"all\"}' | jq || echo 'Server not running or authentication required'",
    "clean": "rm -rf .next && echo '✅ Cleared .next directory'",
    "clean:all": "rm -rf .next node_modules/.cache && echo '✅ Cleared all caches'"
  },
